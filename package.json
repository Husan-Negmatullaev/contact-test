{
	"name": "contact",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"engines": {
		"node": ">=18.12.1",
		"yarn": ">=1.22.19",
		"npm": "USE-YARN"
	},
	"scripts": {
		"dev": "vite",
		"preview": "vite preview",
		"build": "tsc && vite build",
		"server": "json-server --watch server/database.json",
		"formatter": "yarn prettier . --write",
		"formatter:check": "npx prettier . --check",
		"formatter:conflicts": "npx eslint-config-prettier src/app/App.tsx",
		"lint:css": "npx stylelint **/*.css",
		"lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.7",
		"axios": "^1.5.1",
		"clsx": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.16.0",
		"react-transition-group": "^4.4.5"
	},
	"devDependencies": {
		"@svgr/plugin-jsx": "^8.1.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@types/node": "^20.8.6",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-transition-group": "^4.4.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"json-server": "^0.17.4",
		"postcss-lit": "^1.1.0",
		"prettier": "3.0.3",
		"stylelint": "^15.10.3",
		"stylelint-config-standard": "^34.0.0",
		"typescript": "^5.0.2",
		"unplugin-fonts": "^1.0.3",
		"vite": "^4.4.5",
		"vite-plugin-svgr": "^4.1.0"
	},
	"main": "app/main.tsx",
	"repository": "https://github.com/Husan-Negmatullaev/contact-test.git",
